{
  "name": "gnosis",
  "private": true,
  "scripts": {
    "dev": "set NODE_OPTIONS=--max_old_space_size=4096 && export GTM_ENV=dev && next dev",
    "build:dev": "export NODE_ENV=development && export GTM_ENV=dev && next build && next export",
    "build": "export GTM_ENV=prod && next build && next export",
    "start": "next start",
    "lint": "next lint",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook",
    "test": "jest --watch",
    "deploy:dev": "export GTM_ENV=dev && next build && next export && firebase deploy --only=hosting:dev --project=gnosis-webapp",
    "deploy": "export GTM_ENV=prod && next build && next export && firebase deploy --only=hosting:micro --project=gnosis-webapp",
    "deploy:storybook": "build-storybook && firebase deploy --only=hosting:storybook --project=gnosis-webapp",
    "build:analyze": "next build && next export"
  },
  "dependencies": {
    "@emotion/cache": "^11.7.1",
    "@emotion/react": "^11.9.0",
    "@emotion/server": "^11.4.0",
    "@emotion/styled": "^11.8.1",
    "@faker-js/faker": "6.1.2",
    "@hookform/resolvers": "^2.8.8",
    "@mirowsky/mui-stylesheet": "^1.0.7",
    "@mui/icons-material": "^5.6.0",
    "@mui/lab": "^5.0.0-alpha.76",
    "@mui/material": "^5.6.0",
    "@mui/system": "^5.6.0",
    "dotenv": "^14.3.2",
    "framer-motion": "^6.2.9",
    "next": "12.0.8",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-hook-form": "^7.29.0",
    "react-icons": "^4.3.1",
    "react-number-format": "4.7.3",
    "swiper": "8.0.6",
    "webpack": "^5.72.0",
    "yup": "^0.32.11",
    "zustand": "^3.7.2"
  },
  "devDependencies": {
    "@storybook/addon-actions": "^6.4.20",
    "@storybook/addon-essentials": "^6.4.20",
    "@storybook/addon-links": "^6.4.20",
    "@storybook/builder-webpack5": "^6.4.20",
    "@storybook/manager-webpack5": "^6.4.20",
    "@storybook/react": "^6.4.20",
    "@testing-library/jest-dom": "^5.16.4",
    "@testing-library/react": "^12.1.4",
    "@types/jest": "^27.4.1",
    "@types/node": "17.0.9",
    "@types/react": "17.0.38",
    "@types/react-dom": "^17.0.15",
    "csstype": "^3.0.11",
    "eslint": "8.7.0",
    "eslint-config-next": "12.0.8",
    "eslint-plugin-jest-dom": "^4.0.1",
    "eslint-plugin-storybook": "^0.5.8",
    "eslint-plugin-testing-library": "^5.2.1",
    "jest": "^27.5.1",
    "msw": "0.35.0",
    "ts-jest": "^27.1.4",
    "tsconfig-paths-webpack-plugin": "^3.5.2",
    "typescript": "4.5.4"
  },
  "msw": {
    "workerDirectory": "public"
  }
}